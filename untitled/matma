x1 = (loc.latitude)
y1 = (loc.longitude)
x2 = 50.394167
y2 = 18.891667


def calculateDistance(x1, y1, x2, y2):
    dist = math.sqrt((x2 - x1) ** 2 + (y2 - y1) ** 2) * 100
    return dist


print("Odległość: %.2fKM" % float(calculateDistance(x1, y1, x2, y2)))


